#include<iostream>
#include<vector>
using namespace std;
int solve(vector<int> &a,int x){
	int count=0;
	for(int i=0; i<a.size(); i++){
		if(a[i]=x){
			count++;
		}
	}
	return count;
	
}

int main(){
	int n;
	cin>>n;
	int x;
	cin>>x;
	cout<<"Enter the value of x:";
    vector<int> a(n);

	for(int i=0; i<n; i++){
		cin>>a[i];
	
	}cout<<solve(a,x);
	return 0;
}
